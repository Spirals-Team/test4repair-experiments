#!/bin/bash

export PATH="~/defects4j/framework/bin:$PATH"

cp -r ~/projects/chart/chart_${1}/ /tmp/
cd /tmp/chart_${1}/
defects4j compile

cp ~/nopol/nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar .
cp -r ~/nopol/z3-x64-debian-7.7/ .

time java -cp nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar fr.inria.lille.repair.Main --mode repair --type precondition --oracle angelic --source source/ --classpath build/:build-tests/:lib/servlet.jar:nopol-0.0.3-SNAPSHOT-jar-with-dependencies.jar --solver z3 --solver-path z3-x64-debian-7.7/bin/z3

cd /tmp/
rm -rf chart_${1}/
